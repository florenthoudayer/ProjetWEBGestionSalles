<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GestionSalleBundle\Entity\Reservation" table="reservation">
    <indexes>
      <index name="FK_reservation_id_salles" columns="id_salles"/>
      <index name="FK_reservation_id_formation" columns="id_formation"/>
      <index name="FK_reservation_id_matiere" columns="id_matiere"/>
      <index name="FK_reservation_id_utilisateur" columns="id_utilisateur"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateDebut" type="date" column="date_debut" nullable="true"/>
    <field name="dateFin" type="date" column="date_fin" nullable="true"/>
    <many-to-one field="idUtilisateur" target-entity="Utilisateur" fetch="LAZY">
      <join-columns>
        <join-column name="id_utilisateur" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idSalles" target-entity="Salles" fetch="LAZY">
      <join-columns>
        <join-column name="id_salles" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMatiere" target-entity="Matiere" fetch="LAZY">
      <join-columns>
        <join-column name="id_matiere" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idFormation" target-entity="Formation" fetch="LAZY">
      <join-columns>
        <join-column name="id_formation" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
