<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GestionSalleBundle\Entity\Utilisateur" table="utilisateur">
    <indexes>
      <index name="FK_utilisateur_id_formation" columns="id_formation"/>
      <index name="FK_utilisateur_id_titre" columns="id_titre"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mdp" type="string" column="mdp" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="actif" type="boolean" column="actif" nullable="true"/>
    <many-to-one field="idFormation" target-entity="Formation" fetch="LAZY">
      <join-columns>
        <join-column name="id_formation" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTitre" target-entity="Titre" fetch="LAZY">
      <join-columns>
        <join-column name="id_titre" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idMatiere" target-entity="Matiere" mapped-by="idUtilisateur" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
